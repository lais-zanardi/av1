{
  "compilerOptions": {
    "target": "ES5", 
    "module": "CommonJS", 
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "out/",
    "rootDir": "src/",
    "moduleResolution": "node",
  },
  "include": [
    "src/**/*"
  ]
}